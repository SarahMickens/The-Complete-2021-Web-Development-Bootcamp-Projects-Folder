// leap year years are divisble by 4. They are NOT divible by 100 unless it's divisble by 400//

function isLeap(year) {

if (year % 4 === 0) {
    if (year % 100 === 0) {
        if (year % 400 === 0) {
return "Leap year.";
} else {
return "Not leap year.";
} 
} else {
return "Not leap year.";
} 
} else {
return "Not leap year.";
}
}


isLeap();
